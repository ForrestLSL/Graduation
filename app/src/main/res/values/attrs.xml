<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Title属性设置-->
    <declare-styleable name="Title">
        <attr name="text" format="string|reference" />
        <attr name="text_color" format="string|reference" />
        <attr name="res_left_btn" format="reference" />
        <attr name="res_right_btn" format="reference" />
        <attr name="text_left" format="string|reference"></attr>
        <attr name="text_right" format="string|reference"></attr>
        <attr name="text_margin_left" format="dimension|reference"></attr>
        <attr name="text_margin_right" format="dimension|reference"></attr>
    </declare-styleable>
    <!--WaterDropListVie属性设置w-->
    <declare-styleable name="WaterDropView">
        <attr name="max_circle_radius" format="dimension"/>
        <attr name="min_circle_radius" format="dimension"/>
        <attr name="waterdrop_color" format="color"/>
        <attr name="topcircle_x" format="dimension"/>
        <attr name="topcircle_y" format="dimension"/>
        <attr name="bottomcircle_x" format="dimension"/>
        <attr name="bottomcircle_y" format="dimension"/>
    </declare-styleable>
</resources>